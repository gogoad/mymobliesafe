<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#ffffff"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/tv_popup_black_shoudong"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:drawableLeft="@android:drawable/btn_star_big_on"
        android:gravity="center_vertical"
        android:textColor="#000000"
        android:text="@string/manualimport"
        android:textSize="18sp" />

    <TextView
        android:textColor="#000000"
        android:id="@+id/tv_popup_black_contacts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:drawableLeft="@android:drawable/btn_star_big_on"
        android:gravity="center_vertical"
        android:text="@string/contactsimport"
        android:textSize="18sp" />

    <TextView
        android:textColor="#000000"
        android:id="@+id/tv_popup_black_phonelog"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:drawableLeft="@android:drawable/btn_star_big_on"
        android:gravity="center_vertical"
        android:text="@string/calllog"
        android:textSize="18sp" />

    <TextView
        android:textColor="#000000"
        android:id="@+id/tv_popup_black_smslog"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:drawableLeft="@android:drawable/btn_star_big_on"
        android:gravity="center_vertical"
        android:text="@string/smslog"
        android:textSize="18sp" />

</LinearLayout>